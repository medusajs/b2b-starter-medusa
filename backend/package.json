{
  "name": "medusa-b2b-starter",
  "version": "0.0.1",
  "description": "A starter for Medusa B2B projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "migrate": "medusa db:migrate",
    "migrate:create": "medusa migrations create",
    "db:setup": "medusa db:create && medusa db:migrate",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "seed:catalog": "medusa exec ./src/scripts/seed-catalog.ts",
    "seed:catalog-integrated": "medusa exec ./src/scripts/seed-catalog-integrated.ts",
    "seed:simple": "medusa exec ./src/scripts/simple-seed.ts",
    "catalog:sku:gen": "medusa exec ./src/scripts/gen-sku-registry.ts",
    "catalog:validate": "medusa exec ./src/scripts/validate-catalog.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "preflight:windows": "pwsh -File ../infra/scripts/windows/assert-non-admin.ps1",
    "doctor:db": "medusa exec ./src/scripts/check-database-health.ts"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.8.0",
    "@medusajs/cli": "2.8.0",
    "@medusajs/framework": "2.8.0",
    "@medusajs/js-sdk": "2.8.0",
    "@medusajs/medusa": "2.8.0",
    "@medusajs/ui": "4.0.14",
    "@medusajs/workflows-sdk": "2.8.0",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@qdrant/js-client-rest": "^1.15.1",
    "@types/multer": "^2.0.0",
    "awilix": "^8.0.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.13.0",
    "posthog-node": "^4.17.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.8.0",
    "@medusajs/ui-preset": "2.8.0",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.2.11"
  },
  "engines": {
    "node": ">=20"
  }
}
