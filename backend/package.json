{
  "name": "medusa-b2b-starter",
  "version": "0.0.1",
  "description": "A starter for Medusa B2B projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "migrate": "medusa db:migrate",
    "migrate:create": "medusa migrations create",
    "db:setup": "medusa db:create && medusa db:migrate",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "seed:catalog": "medusa exec ./src/scripts/seed-catalog.ts",
    "seed:catalog-integrated": "medusa exec ./src/scripts/seed-catalog-integrated.ts",
    "seed:simple": "medusa exec ./src/scripts/simple-seed.ts",
    "sync:catalog": "medusa exec ./src/scripts/sync-catalog-optimized.ts",
    "sync:images": "ts-node src/scripts/sync-image-mappings.ts",
    "sync:full": "npm run sync:images && npm run sync:catalog",
    "catalog:sku:gen": "medusa exec ./src/scripts/gen-sku-registry.ts",
    "catalog:validate": "medusa exec ./src/scripts/validate-catalog.ts",
    "catalog:price:report": "medusa exec ./src/scripts/generate-prices-report.ts",
    "catalog:price:normalize": "medusa exec ./src/scripts/generate-prices-normalized.ts",
    "catalog:import": "medusa exec ./src/scripts/import-catalog.ts",
    "normalize:catalog": "ts-node src/scripts/normalize-catalog/index.ts",
    "normalize:manufacturers": "ts-node src/scripts/normalize-catalog/01-extract-manufacturers.ts",
    "normalize:skus": "ts-node src/scripts/normalize-catalog/02-generate-skus.ts",
    "normalize:kits": "ts-node src/scripts/normalize-catalog/03-normalize-kits.ts",
    "normalize:price-analysis": "ts-node src/scripts/normalize-catalog/04-price-comparison.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "cross-env TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "cross-env TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "cross-env TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:coverage": "cross-env TEST_TYPE=unit jest --coverage --collectCoverageFrom=src/**/*.ts --coveragePathIgnorePatterns=node_modules dist __tests__ scripts --runInBand",
    "test:pact:provider": "cross-env TEST_TYPE=pact jest --testMatch='**/pact/**/*.pact.test.ts' --testTimeout=60000 --runInBand",
    "test:pact:verify": "npm run test:pact:provider",
    "test:pact:can-i-deploy": "pact-broker can-i-deploy --pacticipant=ysh-backend --version=$(git rev-parse --short HEAD) --to-environment=production --broker-base-url=http://localhost:9292 --broker-username=pact --broker-password=pact",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\"",
    "typecheck": "tsc --noEmit",
    "preflight:windows": "pwsh -File ../infra/scripts/windows/assert-non-admin.ps1",
    "doctor:db": "medusa exec ./src/scripts/check-database-health.ts",
    "setup:publishable-key": "node scripts/setup-publishable-key.js",
    "setup:publishable-key:aws": "node scripts/setup-publishable-key.js --upload",
    "normalize:apis": "tsx scripts/normalize-apis.ts",
    "validate:apis": "tsx scripts/validate-apis.ts"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.10.3",
    "@medusajs/cli": "2.10.3",
    "@medusajs/framework": "2.10.3",
    "@medusajs/fulfillment": "2.10.3",
    "@medusajs/inventory": "2.10.3",
    "@medusajs/js-sdk": "2.10.3",
    "@medusajs/medusa": "2.10.3",
    "@medusajs/stock-location": "2.10.3",
    "@medusajs/types": "2.10.3",
    "@medusajs/ui": "4.0.23",
    "@medusajs/workflows-sdk": "2.10.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@qdrant/js-client-rest": "^1.15.1",
    "@types/multer": "^2.0.0",
    "awilix": "^8.0.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.13.0",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2",
    "posthog-node": "^4.18.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "uuid": "^10.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.10.3",
    "@medusajs/ui-preset": "2.10.3",
    "@mikro-orm/cli": "6.4.3",
    "@pact-foundation/pact": "^16.0.0",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.11",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.1",
    "vite": "^5.4.20"
  },
  "overrides": {
    "zod": "3.23.8"
  },
  "engines": {
    "node": ">=20"
  }
}
