{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "title": "Kit Solar (Bundle/Multi-Part Product) - Medusa Product Schema",
    "description": "Schema padronizado para kits solares usando Inventory Kits pattern do Medusa.js",
    "type": "object",
    "required": [
        "title",
        "handle",
        "status",
        "variants",
        "options"
    ],
    "properties": {
        "title": {
            "type": "string",
            "description": "Nome completo do kit com potência e tipo",
            "example": "Kit Solar Fotovoltaico 5.5kWp Grid-Tie Residencial"
        },
        "subtitle": {
            "type": "string",
            "description": "Descrição curta com potência e componentes principais",
            "example": "5.5kWp - 10 Painéis 550W + Inversor 5kW + Estrutura"
        },
        "handle": {
            "type": "string",
            "description": "Handle único URL-friendly",
            "pattern": "^[a-z0-9]+(?:-[a-z0-9]+)*$",
            "example": "kit-solar-5-5kwp-grid-tie-residencial"
        },
        "description": {
            "type": "string",
            "description": "Descrição detalhada do kit completo com geração estimada"
        },
        "is_giftcard": {
            "type": "boolean",
            "const": false
        },
        "discountable": {
            "type": "boolean",
            "default": true
        },
        "status": {
            "type": "string",
            "enum": [
                "published",
                "draft",
                "archived"
            ],
            "default": "published"
        },
        "external_id": {
            "type": "string",
            "description": "ID externo do distribuidor",
            "example": "fotus_kits_KIT-5.5KWP-GRIDTIE"
        },
        "thumbnail": {
            "type": "string",
            "format": "uri"
        },
        "weight": {
            "type": "number",
            "description": "Peso total estimado do kit em kg",
            "minimum": 0
        },
        "hs_code": {
            "type": "string",
            "description": "NCM do kit ou componente principal",
            "example": "8541.40.16"
        },
        "origin_country": {
            "type": "string",
            "description": "País de origem predominante",
            "example": "CN"
        },
        "options": {
            "type": "array",
            "description": "Opções de variação (Potência, Tipo de Telhado, Voltagem)",
            "items": {
                "type": "object",
                "required": [
                    "title",
                    "values"
                ],
                "properties": {
                    "title": {
                        "type": "string",
                        "enum": [
                            "Potência",
                            "Tipo de Telhado",
                            "Voltagem",
                            "Com Bateria"
                        ]
                    },
                    "values": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }
            },
            "minItems": 1
        },
        "variants": {
            "type": "array",
            "description": "Variantes do kit (cada variante é um bundle)",
            "items": {
                "type": "object",
                "required": [
                    "title",
                    "sku",
                    "prices",
                    "options",
                    "inventory_items"
                ],
                "properties": {
                    "title": {
                        "type": "string",
                        "description": "Título da variante do kit",
                        "example": "5.5kWp / Cerâmico / 220V"
                    },
                    "sku": {
                        "type": "string",
                        "description": "SKU único do kit",
                        "pattern": "^[A-Z0-9-]+$",
                        "example": "KIT-5.5KWP-CERAMICO-220V"
                    },
                    "barcode": {
                        "type": "string"
                    },
                    "allow_backorder": {
                        "type": "boolean",
                        "default": false
                    },
                    "manage_inventory": {
                        "type": "boolean",
                        "default": false,
                        "description": "Para kits, geralmente false pois o estoque é calculado dos componentes"
                    },
                    "weight": {
                        "type": "number"
                    },
                    "options": {
                        "type": "object",
                        "description": "Mapeamento título opção -> valor",
                        "example": {
                            "Potência": "5.5kWp",
                            "Tipo de Telhado": "Cerâmico",
                            "Voltagem": "220V"
                        }
                    },
                    "inventory_items": {
                        "type": "array",
                        "description": "INVENTORY KITS: Array de inventory_item_id dos componentes do bundle",
                        "items": {
                            "type": "object",
                            "required": [
                                "inventory_item_id",
                                "required_quantity"
                            ],
                            "properties": {
                                "inventory_item_id": {
                                    "type": "string",
                                    "description": "ID do inventory item do componente (painel, inversor, estrutura, etc)",
                                    "example": "inv_item_01JABY123"
                                },
                                "required_quantity": {
                                    "type": "integer",
                                    "description": "Quantidade necessária deste componente no kit",
                                    "minimum": 1,
                                    "example": 10
                                }
                            }
                        },
                        "minItems": 1,
                        "example": [
                            {
                                "inventory_item_id": "inv_item_panel_cs_550w",
                                "required_quantity": 10
                            },
                            {
                                "inventory_item_id": "inv_item_inv_growatt_5k",
