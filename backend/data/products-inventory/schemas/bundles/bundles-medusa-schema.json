                        "type": "array",
                        "description": "Preços do kit completo com tiered pricing e rules",
                        "items": {
                            "type": "object",
                            "required": [
                                "currency_code",
                                "amount"
                            ],
                            "properties": {
                                "currency_code": {
                                    "type": "string",
                                    "enum": [
                                        "BRL",
                                        "USD"
                                    ],
                                    "default": "BRL"
                                },
                                "amount": {
                                    "type": "integer",
                                    "description": "Preço total do kit em centavos",
                                    "minimum": 0
                                },
                                "min_quantity": {
                                    "type": "integer",
                                    "minimum": 1
                                },
                                "max_quantity": {
                                    "type": "integer",
                                    "minimum": 1
                                },
                                "rules": {
                                    "type": "object",
                                    "properties": {
                                        "customer.groups.id": {
                                            "type": "string"
                                        },
                                        "region.id": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        },
                        "minItems": 1
                    },
                    "metadata": {
                        "type": "object",
                        "description": "Metadados específicos da variante do kit"
                    }
