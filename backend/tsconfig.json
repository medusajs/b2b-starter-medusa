{
  "compilerOptions": {
    "target": "ES2021",
    "esModuleInterop": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": false,
    "declaration": false,
    "sourceMap": false,
    "inlineSourceMap": true,
    "outDir": "./.medusa/server",
    "rootDir": "./",
    "baseUrl": ".",
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "checkJs": false,
    "downlevelIteration": true,
    "paths": {
      "@starter/types": [
        "./src/types"
      ],
      "@starter/types/*": [
        "./src/types/*"
      ]
    }
  },
  "ts-node": {
    "swc": true
  },
  "include": [
    "**/*",
    ".medusa/types/*"
  ],
  "exclude": [
    "node_modules",
    ".medusa/server",
    ".medusa/admin",
    ".cache",
    "src/scripts/run-api-tests.ts",
    "src/scripts/start-test-server.ts",
    "src/scripts/test-catalog-paths.ts"
  ]
}