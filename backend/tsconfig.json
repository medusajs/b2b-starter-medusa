{
  "compilerOptions": {
    "target": "ES2021",
    "esModuleInterop": true,
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "noEmitOnError": false,
    "declaration": false,
    "sourceMap": false,
    "inlineSourceMap": true,
    "outDir": "./.medusa/server",
    "rootDir": "./",
    "baseUrl": ".",
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "checkJs": false,
    "downlevelIteration": true,
    "paths": {
      "@starter/types": [
        "./src/types"
      ],
      "@starter/types/*": [
        "./src/types/*"
      ],
      "@modules/*": [
        "./src/modules/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@workflows/*": [
        "./src/workflows/*"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@medusajs/medusa/api/utils/define-query-config": [
        "./src/shims/define-query-config.ts"
      ],
      "zod": [
        "./node_modules/@medusajs/utils/node_modules/zod"
      ]
    }
  },
  "ts-node": {
    "swc": true
  },
  "include": [
    "**/*",
    ".medusa/types/*"
  ],
  "exclude": [
    "node_modules",
    ".medusa/server",
    ".medusa/admin",
    ".cache",
    "src/scripts/run-api-tests.ts",
    "src/scripts/start-test-server.ts",
    "src/scripts/test-catalog-paths.ts",
    "**/__tests__/**",
    "**/*.pact.test.ts",
    "src/pact/**",
    "src/scripts/check-prices.ts",
    "src/scripts/create-publishable-key.ts",
    "src/scripts/import-simple.ts",
    "src/scripts/link-products-to-channel.ts",
    "src/scripts/seed-b2b-data.ts",
    "src/api/store/health/**",
    "scripts/setup-publishable-key.ts",
    "data/catalog/data/medusa_integration/**"
  ]
}