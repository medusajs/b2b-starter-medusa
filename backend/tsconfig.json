{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@admin/*": [
        "admin/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@jobs/*": [
        "jobs/*"
      ],
      "@links/*": [
        "links/*"
      ],
      "@modules/*": [
        "modules/*"
      ],
      "@scripts/*": [
        "scripts/*"
      ],
      "@subscribers/*": [
        "subscribers/*"
      ],
      "@workflows/*": [
        "workflows/*"
      ],
      "@types/*": [
        "types/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@medusajs/medusa/api/utils/define-query-config": [
        "./src/shims/define-query-config.ts"
      ]
    },
    "outDir": "dist"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    ".medusa",
    "dist"
  ]
}